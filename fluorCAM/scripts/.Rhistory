df
means <- df %>%
group_by(plant) %>%
dplyr::summarise(weight = mean(weight))
means$error.bars <- c(0.72, 1.42, 1.63, 1.42)
means
seed_plot <- ggplot(df, aes(x = plant, y = weight, fill = plant)) +
